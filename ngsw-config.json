{
  "$schema": "https://angular.io/schemas/service-worker/1.0.0/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/index.html",
          "/favicon.ico",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/manifest.webmanifest"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-produtos",
      "urls": [
        "https://gerenciamento-de-estoque-mw08.onrender.com/produtos/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 200,
        "maxAge": "1h",
        "timeout": "5s"
      }
    },
    {
      "name": "api-categorias",
      "urls": [
        "https://gerenciamento-de-estoque-mw08.onrender.com/categorias/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "7d"
      }
    },
    {
      "name": "api-entregas",
      "urls": [
        "https://gerenciamento-de-estoque-mw08.onrender.com/entregas/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 200,
        "maxAge": "10m",
        "timeout": "2s"
      }
    },
    {
      "name": "api-movimentacoes",
      "urls": [
        "https://gerenciamento-de-estoque-mw08.onrender.com/movimentacoes/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 200,
        "maxAge": "10m",
        "timeout": "2s"
      }
    },
    {
      "name": "api-consumidores",
      "urls": [
        "https://gerenciamento-de-estoque-mw08.onrender.com/consumidores/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 200,
        "maxAge": "1h",
        "timeout": "5s"
      }
    },
    {
      "name": "api-relatorios",
      "urls": [
        "https://gerenciamento-de-estoque-mw08.onrender.com/relatorios/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 50,
        "maxAge": "15m",
        "timeout": "10s"
      }
    },
    {
      "name": "api-orgs",
      "urls": [
        "https://gerenciamento-de-estoque-mw08.onrender.com/api/orgs/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 50,
        "maxAge": "7d"
      }
    },
    {
      "name": "api-roles",
      "urls": [
        "https://gerenciamento-de-estoque-mw08.onrender.com/roles/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 50,
        "maxAge": "7d"
      }
    }
  ]
}